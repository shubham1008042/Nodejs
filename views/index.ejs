<html>
  <head>
    <title>
      AJAX Zapien
    </title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

  <link type="text/css" href="stylesheets/style.css">
<style>
body {
  background-color: #FFFFCC;
}
</style>


  </head>
  <body>
  	
<div class="container-fluid">	
   		<div id="champ">
      	<% if(data>=1){ %>
      	<div class="row">

  <div class="column">
<h1>Room is not available</h1>
<div class="img1">
    <img src="images/conference2.jpg">
  </div>
</div>
</div>
 <% }

  else{ %>  
 <!-- <h3>Information:</h3> -->
  <div class="row">
<div class="column">
 <div class="col-sm-5"> <h1>Conference Room is Available now</h1></div>
<div class="col-sm-5">
<div class="img1">
     <img src="images/conference1.jpg">
</div>
</div>
</div>
<!-- <div class="col-sm-5"><img src="images/conference1.jpg" class="img1"></div> -->
</div>
<% } %>
 
      </div>
</div>

<script type="text/javascript">


var ajax_call=function(){
	$(document).ready(function() {
 $.ajax({
        type: 'GET',
        url: 'http://localhost:3000/show/livedata',
        success: function(data) {
             $('#champ').html(data);
        }
      });
 $.ajaxSetup({cache:true});
});
};
var interval=20000;
setInterval(ajax_call,interval)
</script>


    <script src="https://code.jquery.com/jquery-3.1.1.js" integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA=" crossorigin="anonymous"></script>
  
</body>
</html>